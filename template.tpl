___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const makeString = require('makeString');
const getType = require('getType');
const logToConsole = require('logToConsole');
const createQueue = require('createQueue');
const callInWindow = require('callInWindow');
const injectScript = require('injectScript');
const queryPermission = require('queryPermission');
const encodeUri = require('encodeUri');

const tagType = data.mfTagType;
const _mfq = createQueue('_mfq'); // Create the queue function
const mouseflow = callInWindow('mouseflow');
const pageNotFound = callInWindow('mouseflow.pageNotFound');

switch (tagType) {
  case 'mfTrackingCode':
    const siteId = makeString(data.mfSiteID);
    const scriptUrl = "https://cdn.mouseflow.com/projects/" + encodeUri(siteId) + ".js";

    const onSuccess = function() {
      logToConsole("Mouseflow script loaded successfully.");
      data.gtmOnSuccess();
    };

    const onFailure = function() {
      logToConsole("Failed to load Mouseflow script.");
      data.gtmOnFailure();
    };

    if (queryPermission('inject_script', "https://cdn.mouseflow.com")) {
      injectScript(scriptUrl, onSuccess, onFailure);
    } else {
      data.gtmOnFailure();
    }
    break;

  case 'mfTags':
    const tagValue = makeString(data.mfTagsConfig);
    if (tagValue) {
      _mfq(["tag", tagValue]); // Call the queue function
      logToConsole("Mouseflow Tag Executed:", tagValue);
    }
    break;

  case 'mfCustomVariable':
    const key = makeString(data.mfCustomVairableKey);
    const value = makeString(data.mfCustomVairableValue);
    if (key && value) {
      if (data.mfSessionScope) {
        _mfq(["setVariable", key, value, "session", false]); // Call the queue function
      } else {
        _mfq(["setVariable", key, value]); // Call the queue function
      }
      logToConsole("Mouseflow Custom Variable Set:", key, value);
    }
    break;

  case 'mfUserID':
    const userId = makeString(data.mfUserID);
    if (userId) {
      _mfq(["identify", userId]); // Call the queue function
      logToConsole("Mouseflow User Identified:", userId);
    }
    break;

  case 'mfPageNotFound':
    if (pageNotFound) {
      pageNotFound();
      logToConsole("Mouseflow 404 Friction Triggered");
    }
    break;

  case 'mfCustomFriction':
    const frictionPoint = makeString(data.mfCustomFrictionPoint);
    const frictionName = makeString(data.mfCustomFrictionName);
    if (frictionPoint) {
      _mfq(["addFriction", frictionPoint, frictionName]); // Call the queue function
      logToConsole("Mouseflow Custom Friction Triggered:", frictionPoint, frictionName);
    }
    break;

  case 'mfTrackFormEvents':
    const formId = makeString(data.mfFormId);
    if (formId) {
      switch (data.mfFormInteractionType) {
        case 'mfFormSubmit':
          _mfq(["formSubmitAttempt", formId]); // Call the queue function
          logToConsole("Mouseflow Form Submit Attempt:", formId);
          break;
        case 'mfFormSubmitSuccess':
          _mfq(["formSubmitSuccess", formId]); // Call the queue function
          logToConsole("Mouseflow Form Submit Success:", formId);
          break;
        case 'mfFormFailure':
          _mfq(["formSubmitFailure", formId]); // Call the queue function
          logToConsole("Mouseflow Form Submit Failure:", formId);
          break;
      }
    }
    break;
}

data.gtmOnSuccess();


